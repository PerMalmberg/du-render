{
  "cli": {
    "fmtVersion": 3
  },
  "name": "du-render",
  "description": "RenderScript Framework",
  "sourcePath": "src",
  "outputPath": "out",
  "libs": [],
  "builds": {
    "render_example": {
      "name": "example/render/main",
      "type": "screen",
      "slots": {}
    },
    "stream_example_screen": {
      "name": "example/stream/screen",
      "type": "screen",
      "slots": {}
    },
    "stream_example_controller": {
      "name": "example/stream/board",
      "type": "control",
      "slots": {}
    }
  },
  "targets": {
    "development": {
      "name": "development",
      "minify": false,
      "handleErrors": false
    },
    "release": {
      "name": "release",
      "minify": true,
      "handleErrors": false
    }
  },
  "internalPaths": [
    "autoconf/",
    "cpml/",
    "pl/",
    "utils/event"
  ]
}