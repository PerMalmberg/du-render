            },
            "gauge": {
                "layer": 1,
                "pos": "(15,1)",
                "dimensions": "(10,98)",
                "values": {
                    "font": "Play10",
                    "bind": "fuel/flight/absspeed",
                    "min": "some text"
                }
            }
        },
        "pagename2": {}
    }
