            "font": "Play",
            "size": 10
        },
        "Montserrat5": {
            "font": "Montserrat",
            "size": 5
        }
    },
    "styles": {
        "button1": {
            "align": "h0,v1",
            "stroke": {
                "color": "r2.000,g2.000,b2.000,a0.500",
